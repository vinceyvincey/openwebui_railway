{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "DOCKERFILE",
        "dockerfilePath": "dockerfile"
    },
    "deploy": {
        "startCommand": "docker run -d -p 8080:8080 -v open-webui:/app/backend/data ghcr.io/open-webui/open-webui:main",
        "restartPolicyType": "ON_FAILURE",
        "restartPolicyMaxRetries": 10
    }
} 